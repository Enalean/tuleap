          <description>Who is in charge of this artifact</description>
          <properties size="7"/>
          <bind type="users">
            <items>
              <item label="group_members"/>
            </items>
          </bind>
        </formElement>
      </formElements>
    </formElement>
    <formElement type="fieldset" ID="F22" rank="167">
