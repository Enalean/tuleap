{
    "compilerOptions": {
        "target": "ES2020",
        "module": "es2020",
        "lib": ["ES2020", "ES2021"],
        "sourceMap": true,
        "strict": true,
        "exactOptionalPropertyTypes": true,
        "isolatedModules": true,
        "noImplicitOverride": true,
        "declaration": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "types": [],
        "incremental": true,
        "jsx": "preserve"
    }
}
