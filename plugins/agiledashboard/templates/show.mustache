<div class="agiledashboard">
    <div class="dropdown">
        <h2 class="dropdown-toggle" data-toggle="dropdown">{{milestoneTitle}} <b class="caret"></b></h2>
        {{#startDate}}
        <div class="release-date-info">
            <div>{{startDate}}</div>
        </div>
        {{/startDate}}
        <ul class="dropdown-menu">
            {{#selectableArtifacts}}
            <li><a href="{{url}}">{{{title}}}</a></li>
            {{/selectableArtifacts}}
            <li class="divider"></li>
            <li><a href="{{createNewItemToPlanUrl}}">{{createNewItemToPlan}}</a></li>
        </ul>
    </div>
    <ul class="nav nav-tabs">
        {{#getPaneInfoList}}
            <li class="{{#isActive}}active{{/isActive}}">
                <a href="{{getUri}}">{{getTitle}}</a>
            </li>
        {{/getPaneInfoList}}
    </ul>
    <div class="tab-content">
        <div id="{{getActivePane.getIdentifier}}" class="tab-pane active">
        {{{getActivePane.getFullContent}}}
        </div>
    </div>
</div>

