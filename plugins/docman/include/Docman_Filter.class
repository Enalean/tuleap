class Docman_Filter {
    var $name;
    var $title;        
    var $v;

    function Docman_Filter() {  
        $this->v = null;
    }
  
    function setValue($v) {
        $this->v = $v;
    }

    function getValue() {
        return $this->v;
    }

    function setName($v) {
        $this->name = $v;
    }

    function getName() {
        return $this->name;
    }

    function setTitle($v) {
        $this->title = $v;
    } 

    function getTitle() {
        return $this->title;
    }

    function setTable($v) {
        $this->table = $v;
    } 

    function getTable() {
        return $this->table;
    }

    function setField($v) {
        $this->field = $v;
    } 

    function getField() {
        return $this->field;
    }
}

class Docman_FilterDate extends Docman_Filter {
    var $operator;

    function setOperator($v) {
        $this->operator = $v;
    } 

    function getOperator() {
        return $this->operator;
    }
}

class Docman_FilterOrdering extends Docman_Filter {

    function Docman_FilterOrdering() {
        parent::Docman_Filter();
    }
}

