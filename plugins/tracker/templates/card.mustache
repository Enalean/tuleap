{{#getCardPresenter}}
<div class="card{{ getCssClasses }}"
     {{# hasLegacyAccentColor }}style="background-color: {{ getAccentColor }}"{{/ hasLegacyAccentColor }}
     data-artifact-id="{{getId}}"
     data-csrf-token-challenge-update="{{ csrf_token_challenge_update }}"
>
    <div class="card-container">
        {{> card-actions}}
        <a href="#" class="toggle-collapse">+</a>
        <div class="card-title">{{getTitle}}</div>
        {{> card-details}}
        {{# user_has_accessibility_mode }}<div class="card-accessibility"></div>{{/ user_has_accessibility_mode }}
    </div>
</div>
{{/getCardPresenter}}
