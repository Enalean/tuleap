{{#getCardPresenter}}
<div class="card" style="background-color: {{getAccentColor}};" data-artifact-id="{{getId}}">
    <div class="card-container">
        {{> card-actions}}
        <a href="#" class="toggle-collapse">+</a>
        <span class="card-title">{{getTitle}}</span>
        &nbsp;
        {{> card-details}}
    </div>
</div>
{{/getCardPresenter}}
