{
  "name": "tuleap/plugin-cardwall",
  "autoload": {
    "psr-4": {
      "Tuleap\\Cardwall\\": "include/"
    },
    "classmap": ["include/"]
  },
  "autoload-dev": {
    "psr-4": {
      "Tuleap\\Cardwall\\": ["tests/unit/", "tests/unit/Cardwall"]
    },
    "classmap": ["tests/unit/"]
  },
  "config": {
    "sort-packages": true
  },
  "tuleap": {
    "preload": {
      "include": [
        "/include"
      ]
    }
  }
}
