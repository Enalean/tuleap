<div class="tlp-framed-vertically"
     id="document-tree-view"
     data-project-id="{{ project_id }}"
     data-project-name="{{ project_name }}"
     data-project-public-name="{{ project_public_name }}"
     data-project-url="{{ project_url }}"
     data-user-is-admin="{{ user_is_admin }}"
     data-user-can-create-wiki="{{ user_can_create_wiki }}"
     data-user-can-delete-item="{{ user_can_delete_item }}"
     data-max-files-dragndrop="{{ max_files_dragndrop }}"
     data-max-size-upload="{{ max_size_upload }}"
     data-embedded-are-allowed="{{ embedded_are_allowed }}"
     data-is-item-status-metadata-used="{{ is_item_status_metadata_used }}"
     data-is-obsolescence-date-metadata-used="{{ is_obsolescence_date_metadata_used }}"
     data-csrf-token-name="{{ csrf_token_name }}"
     data-csrf-token="{{ csrf_token }}"
     data-warning-threshold="{{ warning_threshold }}"
     data-max-archive-size="{{ max_archive_size }}"
     data-is-changelog-proposed-after-dnd="{{ is_changelog_proposed_after_dnd }}"
     data-relative-dates-display="{{ relative_dates_display }}"
     data-privacy="{{ privacy }}"
     data-project-flags="{{ project_flags }}"
>
</div>
