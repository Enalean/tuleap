<project unix-name="git-dashboard" full-name="Git dashboard" description="aa" access="public">
  <long-description/>
  <services>
    <service shortname="summary" enabled="1"/>
    <service shortname="admin" enabled="1"/>
    <service shortname="plugin_git" enabled="1"/>
  </services>
  <ugroups>
    <ugroup name="project_members" description="">
      <members>
        <member format="username">ProjectAdministrator</member>
        <member format="username">ProjectMember</member>
      </members>
    </ugroup>
    <ugroup name="project_admins" description="">
      <members>
        <member format="username">ProjectAdministrator</member>
      </members>
    </ugroup>
  </ugroups>
  <dashboards>
    <dashboard name="Dashboard">
      <line>
        <column>
          <widget name="projectmembers" />
        </column>
      </line>
    </dashboard>
  </dashboards>
  <git>
    <repository name="Aquali" description="-- Default description --" allow_artifact_closure="1" default_branch="main" bundle-path="">
      <read>
        <ugroup>ProjectMember</ugroup>
      </read>
      <write>
        <ugroup>ProjectAdministrator</ugroup>
      </write>
      <wplus/>
    </repository>
  </git>
</project>
