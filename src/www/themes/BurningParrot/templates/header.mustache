                        </svg>

                        <p class="sidebar-about-info">{{{ powered_by }}}</p>
                        <p class="sidebar-about-info">{{{ copyright }}}</p>
                    </div>
                </aside>
            {{/ sidebar }}

            <main role="main" class="{{ main_classes }}">
                {{# has_breadcrumbs }}
                    {{> breadcrumbs/breadcrumbs }}
                {{/ has_breadcrumbs }}

                {{# has_toolbar }}
                    <nav class="toolbar">
                        {{# toolbar }}
                            {{{ . }}}
                        {{/ toolbar }}
                    </nav>
                {{/ has_toolbar }}

                {{# has_feedbacks }}
                    <div id="feedback" data-test="feedback">
                        {{# feedbacks }}
                            <section class="tlp-alert-{{ level }}">
                                {{# purified_messages }}
                                    <p>{{{ . }}}</p>
                                {{/ purified_messages }}
                            </section>
                        {{/ feedbacks }}
                    </div>
                {{/ has_feedbacks }}
