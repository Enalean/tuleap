{
  "autoload": {
    "psr-4": {
      "Tuleap\\": ["common/"]
    },
    "classmap": ["common/", "embedded_vendor/"],
    "exclude-from-classmap": ["common/wiki/phpwiki/"]
  },
  "autoload-dev": {
    "psr-4": {
      "Tuleap\\Test\\": ["../tests/lib/"]
    },
    "classmap": ["../tests/lib/"]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Enalean/mustache.php.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/Enalean/Restler.git"
    }
  ],
  "require": {
    "Luracast/Restler": "dev-3.0.0-RC6-July2018-FixURLDecoding",
    "delight-im/cookie": "^3.1",
    "embed/embed": "^3.3",
    "ezyang/htmlpurifier": "^4.10",
    "firebase/php-jwt": "^5.0.0",
    "gumlet/php-image-resize": "^1.9",
    "jimdo/prometheus_client_php": "^0.9.1",
    "michelf/php-markdown": "^1.1",
    "mustache/mustache": "dev-feature/strict-variables",
    "nikic/fast-route": "^1.2",
    "paragonie/easydb": "^1.3",
    "php-amqplib/php-amqplib": "^2.7",
    "php-http/client-common": "^1.7",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message": "^1.6",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "symfony/console": "^3.4",
    "zendframework/zend-mail": "^2.10"
  },
  "config": {
    "platform": {
      "php": "7.2",
      "ext-bcmath": "0",
      "ext-mbstring": "0",
      "ext-curl": "0",
      "ext-iconv": "0",
      "ext-gd": "0"
    },
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "^1.0",
    "simpletest/simpletest": "^1.1",
    "mikey179/vfsStream": "^1.6",
    "php-http/mock-client": "^1.1",
    "squizlabs/php_codesniffer": "^3.3"
  }
}
