 # $Id: Makefile,v 1.47 2005/09/12 06:12:06 rurban Exp $
#
# locale/Makefile
#
# This is a replacement for locale/translate.sh to avoid remaking
# files that don't need to be remade.
#

POT_FILE = po/phpwiki.pot
SYMLINK  = cp
#SYMLINK  = ln -s

.PHONY: all po mo clean depend ${POT_FILE}

all	: po mo
po 	: # dependencies are auto-generated by 'make depend' , see below
mo	: # dependencies are auto-generated by 'make depend' , see below

${POT_FILE} : # dependencies are auto-generated by 'make depend' , see below
	cp -f $@ $@~
	xgettext -L PHP --from-code=utf-8 -k_ -k__printf -k__sprintf -k__vsprintf -kfmt -o $@.extracted $^
	sed -e's,; charset=CHARSET,; charset=utf-8,' -i $@.extracted
	msgmerge -o $@ $@~ $@.extracted && rm -f $@.extracted 

# merge in place to keep unchanged timestamps
po/%.po : ${POT_FILE}
	test -f $@ || touch $@
	cp -f $@ $@~
	msgmerge -o $@.new $@~ $^ && mv -f $@.new $@

# current ja is euc-jp (symlink)
#po/ja.po: po/ja.euc-jp.po
#	$(SYMLINK) po/ja.euc-jp.po po/ja.po

# work on ja.euc-jp.po and create utf-8 from this. changes in utf-8 will get lost!
po/ja.utf-8.po: po/ja.po
	@echo "# Autogenerated file from ja.po" > $@
	@echo "# Any changes here will be lost! Edit ja.po instead" >> $@
	iconv -f euc-jp -t utf-8 $^ |sed -e's,; charset=euc-jp,; charset=utf-8,' >> $@

# current zh is zh_TW.UTF-8 (symlink)
#po/zh.po: po/zh_TW.utf-8.po
#	ln -s po/zh_TW.utf-8.po po/zh.po

%/LC_MESSAGES/phpwiki.mo : po/%.po
	mkdir -p $*/LC_MESSAGES
	msgfmt -o $@ $<

# Generate .php from .mo rather than .po to maximize
# the probability of agreement between .mo and .php files.
%/LC_MESSAGES/phpwiki.php : %/LC_MESSAGES/phpwiki.mo
	msgunfmt $< | awk -f make-php-trans.awk > $@

clean :
	rm -f ${MO_FILES} ${PHP_FILES}

depend :
	$(SHELL) update-makefile.sh

dep :
	@echo "Please use make depend"

# requires gettext-lint into /usr/local
check: 
	POFileChecker po/*.po | xsltproc /usr/local/share/gettext-lint/report.xsl - | \
		perl -pe's/<table>/<table border="1">/; s/<tr><td rowspan=/<tr valign="top"><td rowspan=/g' \
	        > POFileChecker.html

# DO NOT DELETE THIS LINE
#
# The remainder of this file is auto-generated
#
# (Run 'make depend' regenerate this section.)
#
po: po/fr.po
mo: fr/LC_MESSAGES/phpwiki.mo fr/LC_MESSAGES/phpwiki.php
#
