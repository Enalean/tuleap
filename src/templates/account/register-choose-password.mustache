<div id="form_pw-tooltip" style="display: none">
    <p class="robustness">{{ password_robustness }} <span class="password_strategy_good">{{ good }}</span><span class="password_strategy_bad">{{ bad }}</span>
        <img class="password_validators_loading" src="/themes/common/images/ic/spinner-16.gif">
    </p>
    {{# password_strategy_validators }}
    <p class="password_validator_msg_{{ key }}"><i class="fa fa-times password_strategy_bad"></i> {{ description }}</p>
    {{/ password_strategy_validators }}
</div>

<script type="text/javascript">
    var password_validators = {{ json_password_strategy_keys }};
</script>
