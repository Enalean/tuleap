        <h1 class="empty-state-title">{{ no_content }}</h1>
        <a href="/admin/" class="empty-state-action tlp-button-primary">
            {{ go_back }}
            <i class="fas fa-long-arrow-alt-right tlp-button-icon" aria-hidden="true"></i>
        </a>
    </section>
{{/ has_project }}

{{# has_project }}
    {{# pending_projects }}
        {{# project_list }}
            <section class="tlp-pane">
                <div class="tlp-pane-container" id="pending-project-left-pane">
                    <div class="tlp-pane-header">
                        <h1 class="tlp-pane-title siteadmin-pending-project-title">
                            {{ project_public_name }}
                            <small>{{ project_unix_name }}</small>
                            <a href="/admin/groupedit.php?group_id={{ project_id }}"
                               class="tlp-button-primary tlp-button-outline tlp-button-small siteadmin-pending-project-title-button"
                            >
                                {{ see_project_details }}
                            </a>
                        </h1>
                    </div>
                    <section class="tlp-pane-section pending-project-properties">
                        <div class="pending-project-description-and-visibility">
                            <div class="tlp-property">
                                <label class="tlp-label">{{ description_title_label }}</label>
                                <p class="project-visibility-description">{{ project_description }}</p>
                            </div>
                        </div>
                        {{# access_presenter }}
                            <div class="tlp-property pending-project-visibility">
                                <label class="tlp-label">
                                    {{ label_project_visibility }}
                                </label>
                                <p>
                                    <i
                                       class="fas pending-project-access-icon
                                           {{# is_wide_open }}fa-tlp-unlock-plus-r{{/ is_wide_open }}
                                           {{# is_open }}fa-lock-open{{/ is_open }}
                                           {{# is_closed }}fa-lock{{/ is_closed }}
                                           {{# is_closed_incl_restricted }}fa-tlp-lock-plus-r{{/ is_closed_incl_restricted }}
                                    "></i> {{ access }}
                                </p>
                            </div>
                        {{/ access_presenter }}
                        {{# trovecats }}
                            {{# has_categories }}
                            <div class="tlp-property pending-project-trovecats">
                                <label class="tlp-label">{{# gettext }}Trove categories{{/ gettext }}</label>
                                <p>
                                    <i class="fa fa-tags softwaremap-list-results-trovecat-icon"></i>
                                    {{# categories }}
                                        {{ name }}{{^ is_last }}, {{/ is_last }}
                                    {{/categories}}
                                </p>
                            </div>
                            {{/ has_categories }}
                        {{/ trovecats }}
                    </section>
                    {{# has_custom_fields }}
                        <section class="tlp-pane-section">
                            {{# project_fields }}
                                {{# is_required }}
                                    <div class="tlp-property">
                                        <label class="tlp-label">{{ label }}</label>
                                        <p>{{ value }}</p>
                                    </div>
                                {{/ is_required }}
                            {{/ project_fields }}
                        </section>
                    {{/ has_custom_fields }}
                </div>
                <section class="tlp-pane-section">
                    <div class="siteadmin-pending-project-actions">
                        <div>
                            <div class="tlp-property">
                                <label class="tlp-label">{{ submitted_by_label }}</label>
                                {{# admin_is_anonymous }}
                                    <div class="tlp-alert-warning">{{# gettext }}User cannot be found{{/ gettext }}</div>
                                {{/ admin_is_anonymous }}
                                {{^ admin_is_anonymous }}
                                    <div class="pending-project-name-and-avatar">
                                        <a href="/admin/usergroup.php?user_id={{ user_id }}">
                                        <div class="tlp-avatar pending-project-avatar">
                                            {{# user_has_avatar }}
                                                <img src="{{ user_avatar }}">
                                            {{/ user_has_avatar }}
                                        </div>
                                        {{ user_name }}
                                        </a>
                                    </div>
                                {{/ admin_is_anonymous }}
                            </div>
                            <div class="tlp-property">
                                <label class="tlp-label">{{ creation_date_label }}</label>
                                <p>{{ date_creation }}</p>
                            </div>
                        </div>
                        <div class="siteadmin-pending-project-actions-buttons">
                            <form action="" method="POST">
                                {{# csrf_token }}
                                    {{> csrf_token_input }}
                                {{/ csrf_token }}
                                <input type="hidden" name="action" value="activate">
                                <input type="hidden" name="list_of_groups" value="{{ project_id }}">
                                <button
                                        type="submit"
                                        class="tlp-button-success validate-button validate-project"
                                        data-test="validate-project-button"
                                >
                                    <i class="fa fa-check tlp-button-icon"></i> {{ validate_label }}
                                </button>
                            </form>
                            <form action="" method="POST">
                                {{# csrf_token }}
                                    {{> csrf_token_input }}
                                {{/ csrf_token }}
                                <input type="hidden" name="action" value="delete">
                                <input type="hidden" name="group_id" value="{{ project_id }}">
                                <button type="submit" class="tlp-button-danger tlp-button-primary">
                                    <i class="fas fa-trash-alt tlp-button-icon"></i> {{ delete_label }}
                                </button>
                            </form>
                        </div>
                    </div>
                </section>
            </section>
        {{/ project_list}}

        {{# more_than_one_to_validate }}
            <div class="pending-project-buttons-all">
                <form action="" method="POST">
                    {{# csrf_token }}
                        {{> csrf_token_input }}
                    {{/ csrf_token }}
                    <input type="hidden" name="action" value="activate">
                    <input type="hidden" name="list_of_groups" value="{{ projects_id }}">
                    <button type="submit" class="tlp-button-large tlp-button-success">
                        <i class="fa fa-check tlp-button-icon"></i> {{ activate_all_label }}
                    </button>
                </form>
            </div>
        {{/ more_than_one_to_validate }}
    {{/ pending_projects}}
{{/ has_project }}
