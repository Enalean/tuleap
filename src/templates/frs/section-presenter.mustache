<div id="frs-admin-header">
    <h1 class="almost-tlp-title project-header-title">{{ title_frs_administration }}</h1>
    {{# sections }}
        <div class="main-project-tabs">
            <ul class="nav nav-tabs">
                <li {{# permissions_active }}class="active"{{/ permissions_active }}>
                    <a href="{{ permissions_url }}">{{ permissions }}</a>
                </li>
                <li {{# processors_active }}class="active"{{/ processors_active }}>
                    <a href="{{ processors_url }}">{{ processors }}</a>
                </li>
                <li {{# license_agreements_active }}class="active"{{/ license_agreements_active }}>
                    <a href="{{ license_agreements_url }}" data-test="license-agreements">{{# gettext }}License Agreements{{/ gettext }}</a>
                </li>
            </ul>
        </div>
    {{/ sections }}
</div>
