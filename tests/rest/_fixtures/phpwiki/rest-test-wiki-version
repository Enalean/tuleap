<verbatim>\
  <?plugin WikiFormRich action=AppendText buttontext="AddCategory"\
  \	   pulldown[] name=s text="Categories: " value=<!plugin-list TitleSearch s=Category !>\
  \	   ?>\
</verbatim>\
